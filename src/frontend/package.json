{
  "name": "deiwed-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --single-quote --write \"**/**/*.{ts,js,css,scss,vue}\"",
    "start": "vue-cli-service serve --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "axios": "^0.21.1",
    "material-design-icons-iconfont": "^6.7.0",
    "purgecss-whitelister": "^2.4.0",
    "typeface-roboto": "^1.1.13",
    "vue": "^2.6.10",
    "vue-class-component": "^7.2.2",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-eslint-parser": "^7.1.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.4.3",
    "vuetify": "^2.3.8",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@mdi/font": "^7.0.96",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-plugin-pwa": "^5.0.8",
    "@vue/cli-plugin-router": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/runtime-dom": "^3.2.41",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "glob-all": "^3.3.0",
    "prettier": "^2.7.1",
    "purgecss-webpack-plugin": "^5.0.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "typescript": "^4.8.4",
    "vuetify-loader": "^1.9.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": 0,
      "error": "off",
      "no-debugger": 0,
      "quotes": [
        "error",
        "single"
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.13.0"
  }
}
